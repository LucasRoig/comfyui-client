{
  "name": "comfyui-client",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=web-react-router... --filter --filter=!./design-system/*",
    "dev": "turbo run dev --filter=web-react-router... --filter=!./design-system/*",
    "lint": "pnpm biome ci",
    "fix": "pnpm biome check --write",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "coverage": "turbo run coverage",
    "knip": "knip",
    "db:migrate": "pnpm --dir packages/database exec prisma migrate",
    "db:generate": "pnpm --dir packages/database exec prisma generate"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@types/node": "^24.6.2",
    "knip": "^5.64.1",
    "turbo": "^2.5.8",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=22"
  }
}
